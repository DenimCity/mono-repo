{
      "private": true,
      "name": "mono-repo",
      "workspaces": [
            "packages/*"
      ],
       "scripts": {
       "client": "yarn workspace web start",
       "server": "yarn workspace server start",
       "start": "concurrently --kill-others-on-fail \"yarn server\"  \"yarn client\"",
       "rimraf": "rm -rf package/*/node_modules && yarn install"
      },
      "dependencies": {
            "concurrently": "^4.1.1"
      }
}